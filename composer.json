{
    "name": "lsascha/wm-pagepath",
    "type": "typo3-cms-extension",
    "description": "Generates Frontend URL from Backend",
    "authors": [
        {
            "name": "Sascha L\u00f6ffler",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^8.7.1"
    },
    "autoload": {
        "psr-4": {
            "Webmatch\\WmPagepath\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webmatch\\WmPagepath\\Tests\\": "Tests"
        }
    },
    "replace": {
        "wm_pagepath": "self.version",
        "typo3-ter/wm-pagepath": "self.version"
    }
}
